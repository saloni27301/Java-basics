package com.harman.ui;

public class ArrayDemo {

	public static void main(String[] args) {
		int[] arr;
		arr = new int[5];
		
		arr[0] = 10;
		arr[1] = 11;
		arr[2] = 20;
		arr[3] = 17;
		arr[4] = 18;

		for(int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		System.out.println("----------------------------------------------");
		for(int data : arr) {
			System.out.println(data);
		}
		System.out.println("----------------------------------------------");
		
		

	}

}
