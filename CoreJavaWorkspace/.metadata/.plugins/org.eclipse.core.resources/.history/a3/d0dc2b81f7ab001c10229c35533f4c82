package com.harman.model;

public class ComplexNumber {
	/* data members */
	double realPart;
	double imaginaryPart;
	
	/* methods */
	void setRealPart(double real) {
		realPart = real;
	}
	void setImaginaryPart(double imag) {
		imaginaryPart = imag;
	}
	double getRealPart() {
		return realPart;
	}
	double getImaginaryPart() {
		return imaginaryPart;
	}
}
