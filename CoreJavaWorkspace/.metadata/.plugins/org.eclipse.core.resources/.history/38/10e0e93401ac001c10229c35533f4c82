package com.harman.ui;

import com.harman.model.Product;

public class ProductApp {

	public static void main(String[] args) {
		Product.setTax(12);
		
		Product p1 = new Product();
		p1.setId(1);
		p1.setName("TV");
		
		System.out.println(p1.getId() + "\t" + p1.getName() + "\t" + p1.getTax());
		
		

	}

}
